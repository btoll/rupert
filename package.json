{
  "name": "rupert-fp",
  "version": "0.2.3",
  "description": "Static analysis of functional patterns in JavaScript",
  "bin": {
    "rupert-fp": "./bin/rupert-fp"
  },
  "main": "./src/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "clean": "rm -rf build coverage",
    "compile": "babel -d build src",
    "coverage": "JASMINE_CONFIG_PATH=jasmine.json istanbul cover jasmine --captureExceptions",
    "coveralls": "npm run coverage && cat ./coverage/lcov.info | coveralls && rm -rf ./coverage",
    "lint": "eslint bin spec src",
    "specs": "JASMINE_CONFIG_PATH=jasmine.json jasmine",
    "test": "npm run clean && npm run lint && npm run compile && npm run specs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/btoll/rupert-fp.git"
  },
  "keywords": [
    "functional programming",
    "FP",
    "static code analysis",
    "static analysis",
    "Esprima"
  ],
  "author": "Benjamin Toll <benjam72@yahoo.com> (http://www.benjamintoll.com)",
  "license": "GPLv3",
  "bugs": {
    "url": "https://github.com/btoll/rupert-fp/issues"
  },
  "homepage": "https://github.com/btoll/rupert-fp#readme",
  "dependencies": {
    "glob": "^7.0.3",
    "onf-logger": "^0.2.0",
    "onf-static": "git+https://github.com/btoll/onf-static.git",
    "yargs": "^4.6.0"
  },
  "devDependencies": {
    "babel-core": "^6.7.2",
    "babel-preset-es2015": "^6.6.0",
    "coveralls": "^2.11.8",
    "eslint": "^2.4.0",
    "istanbul": "^0.4.2",
    "jasmine-core": "^2.4.1"
  }
}
