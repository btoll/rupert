{
  "name": "rupert",
  "version": "0.1.0",
  "description": "Static analysis of functional pattern optimization in JavaScript",
  "bin": {
    "rupert": "./bin/rupert"
  },
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "clean": "rm -rf build coverage",
    "compile": "babel -d build src",
    "coverage": "JASMINE_CONFIG_PATH=jasmine.json istanbul cover jasmine --captureExceptions",
    "coveralls": "npm run coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "lint": "eslint bin spec src",
    "specs": "JASMINE_CONFIG_PATH=jasmine.json jasmine",
    "test": "npm run clean && npm run lint && npm run compile && npm run specs",
    "watch": "bash ./scripts/watch.bash"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/btoll/rupert.git"
  },
  "keywords": [
    "functional programming",
    "FP",
    "Esprima"
  ],
  "author": "Benjamin Toll <benjam72@yahoo.com> (http://www.benjamintoll.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/btoll/rupert/issues"
  },
  "homepage": "https://github.com/btoll/rupert#readme",
  "dependencies": {
    "esprima": "^2.7.0",
    "yargs": "^3.30.0"
  },
  "devDependencies": {
    "babel-core": "^6.7.2",
    "babel-preset-es2015": "^6.6.0",
    "coveralls": "^2.11.8",
    "eslint": "^2.4.0",
    "istanbul": "^0.4.2",
    "jasmine-core": "^2.4.1"
  }
}
